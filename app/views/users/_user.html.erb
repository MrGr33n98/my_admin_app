<div id="<%= dom_id user %>">
  <p>
    <strong>Name:</strong>
    <%= user.name %>
  </p>

  <p>
    <strong>Email:</strong>
    <%= user.email %>
  </p>

  <p>
    <strong>Primary phone:</strong>
    <%= user.primary_phone %>
  </p>

  <p>
    <strong>Secondary phone:</strong>
    <%= user.secondary_phone %>
  </p>

  <p>
    <strong>Company name:</strong>
    <%= user.company_name %>
  </p>

  <p>
    <strong>Admin:</strong>
    <%= user.admin %>
  </p>

  <p>
    <strong>Email confirmed:</strong>
    <%= user.email_confirmed %>
  </p>

</div>
