<div id="<%= dom_id lead %>">
  <p>
    <strong>Company:</strong>
    <%= lead.company_id %>
  </p>

  <p>
    <strong>Lead type:</strong>
    <%= lead.lead_type %>
  </p>

  <p>
    <strong>Client name:</strong>
    <%= lead.client_name %>
  </p>

  <p>
    <strong>Client email:</strong>
    <%= lead.client_email %>
  </p>

  <p>
    <strong>Client phone:</strong>
    <%= lead.client_phone %>
  </p>

  <p>
    <strong>Date:</strong>
    <%= lead.date %>
  </p>

  <p>
    <strong>Value:</strong>
    <%= lead.value %>
  </p>

</div>
